{
  "executionRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/transcribe-dev",
          "awslogs-region": "<AWS_REGION>",
          "awslogs-stream-prefix": "ecs",
          "awslogs-create-group": "true"
        }
      },
      "portMappings": [
        {
          "hostPort": 22,
          "protocol": "tcp",
          "containerPort": 22
        }
      ],
      "cpu": 0,
      "secrets": [
        {
          "name": "AWS_GW_WS",
          "valueFrom": "arn:aws:ssm:<AWS_REGION>:<AWS_ACCOUNT_ID>:parameter/ivs/dev/aws_gw_ws"
        },
        {
          "name": "AWS_IVS_CHANNEL_ARN",
          "valueFrom": "arn:aws:ssm:<AWS_REGION>:<AWS_ACCOUNT_ID>:parameter/ivs/dev/aws_ivs_channel_arn"
        },
        {
          "name": "RTMP_INPUT",
          "valueFrom": "arn:aws:ssm:<AWS_REGION>:<AWS_ACCOUNT_ID>:parameter/ivs/dev/rtmp_input"
        },
        {
          "name": "SSH_PUBLIC_KEY",
          "valueFrom": "arn:aws:ssm:<AWS_REGION>:<AWS_ACCOUNT_ID>:parameter/ivs/dev/ssh_public_key"
        }
      ],
      "environment": [
        {
          "name": "AWS_REGION",
          "value": "<AWS_REGION>"
        },
        {
          "name": "LANGUAGE_CODE",
          "value": "en-US"
        },
        {
          "name": "VOCABULARY_NAME",
          "value": "ivs-en-us-custom-vocabulary-dev"
        },
        {
          "name": "VOCABULARY_FILTER",
          "value": "ivs-en-us-vocabulary-filter-dev"
        }
      ],
      "memoryReservation": 512,
      "volumesFrom": [],
      "image": "<AWS_ACCOUNT_ID>.dkr.ecr.<AWS_REGION>.amazonaws.com/ivs-transcribe-dev",
      "name": "transcribe-dev"
    }
  ],
  "memory": "1024",
  "taskRoleArn": "arn:aws:iam::<AWS_ACCOUNT_ID>:role/ECSTranscriberTaskIAMRole",
  "family": "transcribe-task-dev",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "512"
}
